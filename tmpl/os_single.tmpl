<body>

<TMPL_INCLUDE NAME="nav.ssi">

<table class="form">
<tr>
	<th class="form">
		<a href="<TMPL_VAR NAME=base_url>?view=os&amp;view_type=create"><img style="float:right; margin-left: 4px;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/new.gif" width="47" height="21" alt="New" /></a>
		<a href="<TMPL_VAR NAME=base_url>?view=os"><img style="float:right; margin-left: 4px;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/view_all.gif" width="55" height="21" alt="View All" /></a>
		operating systems
	</th>
</tr>
<tr>
	<td class="form">
		<label>Name</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=name>
		<br /><br />
		
		<label>Manufacturer</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=manufacturer_name> &nbsp; <a class="small" title="View details for this organisation" href="<TMPL_VAR NAME=base_url>?view=org&amp;view_type=single&amp;id=<TMPL_VAR NAME=manufacturer>">Details&hellip;</a>
		<br /><br />
		
		<label>Notes</label><br />
		<TMPL_IF NAME=notes><TMPL_VAR ESCAPE=HTML NAME=notes><TMPL_ELSE>&nbsp;-</TMPL_IF>
		<hr/>
		
		<TMPL_UNLESS NAME=meta_default_data><a href="<TMPL_VAR NAME=base_url>?view=os&amp;view_type=default&amp;act=delete&amp;act_on=os&amp;act_id=<TMPL_VAR NAME=id>"><img style="float:left;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/delete.gif" width="48" height="21" alt="Delete" /></a></TMPL_UNLESS>
		<a href="<TMPL_VAR NAME=base_url>?view=os&amp;view_type=edit&amp;id=<TMPL_VAR NAME=id>"><img style="float:right;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/edit.gif" width="48" height="21" alt="Edit" /></a>

		<p class="updated">Entry last updated by <TMPL_VAR ESCAPE=HTML NAME=meta_update_user> at <TMPL_VAR ESCAPE=HTML NAME=meta_update_time> GMT</p>
	</td>
</tr>
</table>

