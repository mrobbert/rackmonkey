<head>
	<title>Create Operating System | RackMonkey</title>
		<style type="text/css" media="screen">
		@import "<TMPL_VAR NAME=web_root>/styles/main.css";
		@import "<TMPL_VAR NAME=web_root>/styles/tabs.css";
		@import "<TMPL_VAR NAME=web_root>/styles/form.css";
		</style>
		<script type="text/javascript" src="<TMPL_VAR NAME=web_root>/javascript/racklib.js"></script>
		<link rel="icon" href="<TMPL_VAR NAME=web_root>/graphics/favicon.ico" />
	</head>
	<body>
		<TMPL_INCLUDE NAME="nav-config.ssi">
		<div id="main">
			<form method="post" action="<TMPL_VAR NAME=base_url>">
				<table class="form">
					<tr>
						<th><span class="title">Create Operating System</span></th>
					</tr>
					<tr>
						<td>
							<TMPL_INCLUDE NAME="os-edit.ssi">
						</td>
					</tr>
					<tr>
						<td class="footer">		
							<button style="float: left" title="Cancel creating this operating system" onclick="window.location = '<TMPL_VAR NAME=base_url>?view=os&amp;view_type=default'; return false;">Cancel</button>
							<button style="float: right" title="Save this operating system" type="submit">Save</button>	
							<input type="hidden" name="view" value="os" />
							<input type="hidden" name="view_type" value="single" />
							<input type="hidden" name="act" value="insert" />
							<input type="hidden" name="act_on" value="os" />
							&nbsp;
						</td>
					</tr>
				</table>
			</form>
		</div>
		<TMPL_INCLUDE NAME="footer.ssi">
	</body>
