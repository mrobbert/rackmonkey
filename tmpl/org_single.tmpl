<head>
	<title><TMPL_VAR ESCAPE=HTML NAME=name> | RackMonkey</title>
	<link rel="stylesheet" type="text/css" title="Monkey Style" href="<TMPL_VAR NAME=web_root>/styles/rackmonkey.css" />
	<script type="text/javascript" src="<TMPL_VAR NAME=web_root>/javascript/racklib.js"></script>
</head>

<body>

<TMPL_INCLUDE NAME="nav.ssi">

<table class="form">
<tr>
	<th class="form">
		<a href="<TMPL_VAR NAME=base_url>?view=org&amp;view_type=create"><img style="float:right; margin-left: 4px;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/new.gif" width="47" height="21" alt="New" /></a>
		<a href="<TMPL_VAR NAME=base_url>?view=org"><img style="float:right; margin-left: 4px;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/view_all.gif" width="55" height="21" alt="View All" /></a>
		organisations
	</th>
</tr>
<tr>
	<td class="form">
		<label>Name</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=name>
		<br /><br />
		
		<label>Description</label><br />
		<TMPL_IF NAME=descript><TMPL_VAR ESCAPE=HTML NAME=descript><TMPL_ELSE>-</TMPL_IF>
		<br /><br />		

		<label>Account No.</label><br />
		<TMPL_IF NAME=account_no><TMPL_VAR ESCAPE=HTML NAME=account_no><TMPL_ELSE>&nbsp;-</TMPL_IF>
		<br /><br />

		<label>Customer</label><br />
		<input disabled="disabled" type="checkbox" <TMPL_IF NAME=customer>checked="checked"</TMPL_IF> /> 
		<TMPL_IF NAME=customer> &nbsp; <a class="small" title="View devices for this customer" href="<TMPL_VAR NAME=base_url>?view=device&amp;filter_device_customer=<TMPL_VAR ESCAPE=URL NAME=id>">Customer Devices&hellip;</a></TMPL_IF>
		<br /><br />	
		
		<label>Makes Software</label><br />
		<input disabled="disabled" type="checkbox" <TMPL_IF NAME=software>checked="checked"</TMPL_IF> />
		<br /><br />
		
		<label>Makes Hardware</label><br />
		<input disabled="disabled" type="checkbox" <TMPL_IF NAME=hardware>checked="checked"</TMPL_IF> />
		<br /><br />
		
		<label>Home Page</label><br />
		<TMPL_IF NAME=home_page><a href="<TMPL_VAR ESCAPE=HTML NAME=home_page>"><TMPL_VAR ESCAPE=HTML NAME=home_page></a><TMPL_ELSE>&nbsp;-</TMPL_IF>
		
		<TMPL_IF NAME=notes>
		<br /><br />
		<label>Notes</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=notes>
		</TMPL_IF>		
		
		<hr/>
		
		<TMPL_UNLESS NAME=meta_default_data><a href="<TMPL_VAR NAME=base_url>?view=org&amp;view_type=default&amp;act=delete&amp;act_on=org&amp;act_id=<TMPL_VAR ESCAPE=URL NAME=id>"><img style="float:left;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/delete.gif" width="48" height="21" alt="Delete" /></a></TMPL_UNLESS>
		<a href="<TMPL_VAR NAME=base_url>?view=org&amp;view_type=edit&amp;id=<TMPL_VAR ESCAPE=URL NAME=id>"><img style="float:right;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/edit.gif" width="48" height="21" alt="Edit" /></a>		

		<p class="updated">Entry last updated by <TMPL_VAR ESCAPE=HTML NAME=meta_update_user> at <TMPL_VAR ESCAPE=HTML NAME=meta_update_time> GMT</p>
	</td>
</tr>
</table>

<TMPL_INCLUDE NAME="footer.ssi">

</body>
</html>
