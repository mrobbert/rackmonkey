<head>
	<title>Create App | RackMonkey</title>
	<style type="text/css" media="screen">
	@import "<TMPL_VAR NAME=web_root>/styles/main.css";
	@import "<TMPL_VAR NAME=web_root>/styles/tabs.css";
	@import "<TMPL_VAR NAME=web_root>/styles/form.css";
	</style>
	<script type="text/javascript" src="<TMPL_VAR NAME=web_root>/javascript/racklib.js"></script>
</head>
<body>
	<TMPL_INCLUDE NAME="nav-app.ssi">
	<div id="main">
		<form  method="post" action="<TMPL_VAR NAME=base_url>">
			<table class="form">
				<tr>
					<th class="form">Create App</th>
				</tr>
				<tr>
					<td>
						<TMPL_INCLUDE NAME="app-edit.ssi">
					</td>
				</tr>
				<tr>
					<td class="footer">
						<TMPL_IF NAME=return_view>
						<input type="hidden" name="view" value="<TMPL_VAR ESCAPE=HTML NAME=return_view>" />
						<input type="hidden" name="view_type" value="<TMPL_VAR ESCAPE=HTML NAME=return_view_type>" />
						<input type="hidden" name="id" value="<TMPL_VAR ESCAPE=HTML NAME=return_view_id>" />
						<button style="float: left" title="Cancel creating this app" onclick="window.location = '<TMPL_VAR NAME=base_url>?view=<TMPL_VAR ESCAPE=URL NAME=return_view>&amp;view_type=<TMPL_VAR ESCAPE=URL NAME=return_view_type>&amp;id=<TMPL_VAR ESCAPE=URL NAME=return_view_id>'; return false;">Cancel</button>
						<TMPL_ELSE>
						<input type="hidden" name="view" value="app" />
						<input type="hidden" name="view_type" value="single" />
						<button style="float: left" title="Cancel creating this app" onclick="window.location = '<TMPL_VAR NAME=base_url>?view=app&amp;view_type=default'; return false;">Cancel</button>
						</TMPL_IF>

						<input type="hidden" name="act" value="insert" />
						<input type="hidden" name="act_on" value="app" />
						<button style="float: right" title="Save this app" type="submit">Save</button>
						&nbsp;
					</td>
				</tr>
			</table>
		</form>
	</div>
	<TMPL_INCLUDE NAME="footer.ssi">
</body>
</html>