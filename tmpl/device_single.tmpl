<head>
	<title><TMPL_VAR ESCAPE=HTML NAME=name><TMPL_UNLESS NAME=domain_meta_default_data>.<TMPL_VAR ESCAPE=HTML NAME=domain_name></TMPL_UNLESS> | RackMonkey</title>
	<style type="text/css" media="screen">
		@import "<TMPL_VAR NAME=web_root>/styles/main.css";
		@import "<TMPL_VAR NAME=web_root>/styles/tabs.css";
		@import "<TMPL_VAR NAME=web_root>/styles/form.css";
	</style>
	<script type="text/javascript" src="<TMPL_VAR NAME=web_root>/javascript/racklib.js"></script>
</head>

<body>

<TMPL_INCLUDE NAME="nav-device.ssi">

<div id="main">
<table class="form">
<tr>
	<th>
		<a href="<TMPL_VAR NAME=base_url>?view=device&amp;view_type=create"><img style="float:right; margin-left: 4px;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/new.gif" width="47" height="21" alt="New" /></a>
		<a href="<TMPL_VAR NAME=base_url>?view=device"><img style="float:right; margin-left: 4px;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/view_all.gif" width="55" height="21" alt="View All" /></a>
		devices
	</th>
</tr>
<tr>
	<td class="form">
		<div>
		<label>Name</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=name><TMPL_UNLESS NAME=domain_meta_default_data>.<TMPL_VAR ESCAPE=HTML NAME=domain_name></TMPL_UNLESS>
		</div>
		
		<hr />
	
		<div>
		<label>Location</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=rack_name><TMPL_UNLESS NAME=building_meta_default_data> [<TMPL_VAR ESCAPE=HTML NAME=rack_pos>] - <TMPL_VAR ESCAPE=HTML NAME=room_name> in <TMPL_VAR ESCAPE=HTML NAME=building_name> &nbsp; <a class="small" title="View device in rack" href="<TMPL_VAR NAME=base_url>?view=rack&amp;view_type=physical&amp;id=<TMPL_VAR ESCAPE=URL NAME=rack>&amp;device_id=<TMPL_VAR ESCAPE=URL NAME=id>">View in rack&hellip;</a><TMPL_ELSE>&nbsp; <a class="small" title="View unracked devices" href="<TMPL_VAR NAME=base_url>?view=device&amp;view_type=default_unracked">View all unracked devices&hellip;</a></TMPL_UNLESS>
		</div>
		
		<hr />
		
		<div style="clear: both; float: left; width: 280px;">
		<label>Hardware (Size)</label><br />
		<TMPL_UNLESS NAME=hardware_meta_default_data><TMPL_UNLESS NAME=hardware_manufacturer_meta_default_data><TMPL_VAR ESCAPE=HTML NAME=hardware_manufacturer_name> </TMPL_UNLESS></TMPL_UNLESS><TMPL_VAR ESCAPE=HTML NAME=hardware_name> (<TMPL_VAR ESCAPE=HTML NAME=hardware_size>U)<TMPL_UNLESS NAME=hardware_meta_default_data> &nbsp; <a class="small" title="View details for this hardware" href="<TMPL_VAR NAME=base_url>?view=hardware&amp;view_type=single&amp;id=<TMPL_VAR ESCAPE=URL NAME=hardware>">Details&hellip;</a></TMPL_UNLESS>
		</div>
		
		<div>
		<label>Operating System</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=os_name><TMPL_IF NAME=os_version> <TMPL_VAR ESCAPE=HTML NAME=os_version></TMPL_IF><TMPL_UNLESS NAME=os_meta_default_data> &nbsp; <a class="small" title="View details for this operating system" href="<TMPL_VAR NAME=base_url>?view=os&amp;view_type=single&amp;id=<TMPL_VAR ESCAPE=URL NAME=os>">Details&hellip;</a></TMPL_UNLESS>
		</div>
		
		<div style="clear: both; float: left; width: 280px; margin-top: 8px;">
		<label>Role (In Service)</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=role_name>  (<input disabled="disabled" type="checkbox" <TMPL_IF NAME=in_service>checked="checked"</TMPL_IF> />)<TMPL_UNLESS NAME=role_meta_default_data> &nbsp; <a class="small" title="View details for this role" href="<TMPL_VAR NAME=base_url>?view=role&amp;view_type=single&amp;id=<TMPL_VAR ESCAPE=URL NAME=role>">Details&hellip;</a></TMPL_UNLESS>
		</div>
		
		<div style="margin-top: 8px;">
		<label>Purchased (Age)</label><br />
		<TMPL_IF NAME=purchased><TMPL_VAR ESCAPE=HTML NAME=purchased><TMPL_IF NAME=age>&nbsp;&nbsp;(<TMPL_VAR ESCAPE=HTML NAME=age> years)</TMPL_IF><TMPL_ELSE>&nbsp;-</TMPL_IF>
		</div>

		<div style="clear: both; float: left; width: 280px; margin-top: 8px;">
		<label>Serial Number</label><br />
		<TMPL_IF NAME=serial_no><TMPL_VAR ESCAPE=HTML NAME=serial_no><TMPL_IF NAME=dell_query> <a class="small" href="<TMPL_VAR ESCAPE=HTML NAME=dell_query><TMPL_VAR ESCAPE=URL NAME=serial_no>">Query Dell&hellip;</a></TMPL_IF><TMPL_ELSE>&nbsp;-</TMPL_IF>
		</div>
		
		<div style="margin-top: 8px;">
		<label>Asset Number</label><br />
		<TMPL_IF NAME=asset_no><TMPL_VAR ESCAPE=HTML NAME=asset_no><TMPL_ELSE>&nbsp;-</TMPL_IF>
		</div>		
		
		<div style="clear: both; float: left; width: 280px; margin-top: 8px;">
		<label>Customer</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=customer_name><TMPL_UNLESS NAME=customer_meta_default_data> &nbsp; <a class="small" title="View details for this customer" href="<TMPL_VAR NAME=base_url>?view=org&amp;view_type=single&amp;id=<TMPL_VAR ESCAPE=URL NAME=customer>">Details&hellip;</a></TMPL_UNLESS>
		</div>	

		<div style="margin-top: 8px;">
		<label>Service Level</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=service_name><TMPL_UNLESS NAME=service_meta_default_data> &nbsp; <a class="small" title="View details for this service level" href="<TMPL_VAR NAME=base_url>?view=service&amp;view_type=single&amp;id=<TMPL_VAR ESCAPE=URL NAME=service>">Details&hellip;</a></TMPL_UNLESS>
		</div>

		<hr />
		
		<label>Apps</label><br />
		<TMPL_IF NAME=apps>
		<TMPL_LOOP NAME=apps>
		<a title="View details for this app" href="<TMPL_VAR NAME=base_url>?view=app&amp;view_type=single&amp;id=<TMPL_VAR ESCAPE=URL NAME=app_id>"><TMPL_VAR ESCAPE=HTML NAME=app_name></a><TMPL_UNLESS __last__>,</TMPL_UNLESS>
		</TMPL_LOOP>
		<TMPL_ELSE>No apps are hosted on this device.</TMPL_IF>
		
		<hr />
		
		<div style="clear: both;">
		<TMPL_IF NAME=notes>
		<label>Notes</label><br />
		<TMPL_VAR ESCAPE=HTML NAME=notes>
		<hr/>
		</TMPL_IF>
		<TMPL_UNLESS NAME=meta_default_data><a href="<TMPL_VAR NAME=base_url>?view=device&amp;view_type=default&amp;act=delete&amp;act_on=device&amp;act_id=<TMPL_VAR ESCAPE=URL NAME=id>"><img style="float:left;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/delete.gif" width="48" height="21" alt="Delete" /></a></TMPL_UNLESS>
		<a href="<TMPL_VAR NAME=base_url>?view=device&amp;view_type=edit&amp;id=<TMPL_VAR ESCAPE=URL NAME=id>"><img style="float:right;" src="<TMPL_VAR NAME=web_root>/graphics/buttons/edit.gif" width="48" height="21" alt="Edit" /></a>

		<p class="updated">Entry last updated by <TMPL_VAR ESCAPE=HTML NAME=meta_update_user> at <TMPL_VAR ESCAPE=HTML NAME=meta_update_time> GMT</p>
		</div>
	</td>
</tr>
</table>
</div>

<TMPL_INCLUDE NAME="footer.ssi">
</body>
</html>
